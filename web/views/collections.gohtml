<html lang="en">
  {{ template "header.html" }}
  <body>
    {{ template "collectionDisplay" . }} {{ template "collectionForm" . }}
    {{ template "footer.html" }}
  </body>
</html>
{{ block "collectionDisplay" . }}
  <h1>Collections</h1>
  {{ range . }}
    <p>{{ .Name }}</p>
  {{ end }}
{{ end }}
{{ block "collectionForm" . }}
  <form>
    Name:
    <input type="text" name="name" />
    <button>Add Collection</button>
  </form>
{{ end }}
